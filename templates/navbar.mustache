{{!
    Page navbar.
}}
<nav class="navbar bg-white navbar-light" aria-label="{{#str}}sitemenubar, admin{{/str}}">
        <a href="{{{ config.wwwroot }}}" class="navbar-brand {{# output.has_logo }}has-logo{{/ output.has_logo }}
            {{^ output.has_logo }}
                d-none d-sm-inline
            {{/ output.has_logo }}
                ">
            {{# output.has_logo }}
                <span class="logo d-none d-sm-inline">
                    <img src="{{output.get_logo_url}}" alt="{{sitename}}">
                </span>
            {{/ output.has_logo }}
            {{^ output.has_logo }}
                <span class="site-name d-none d-md-inline">{{{ sitename }}}</span>
            {{/ output.has_logo }}
        </a>

        <ul class="navbar-nav d-none d-md-flex">
            <!-- custom_menu -->
            {{{ output.custom_menu }}}
            <!-- page_heading_menu -->
            {{{ output.page_heading_menu }}}
        </ul>
        <ul class="nav navbar-nav ml-auto">
            <!-- navbar_plugin_output -->
            <li class="nav-item">
                {{{ output.navbar_plugin_output }}}
            </li>
            <!-- user_menu -->
            <li class="nav-item d-flex align-items-center">
                {{{ output.user_menu }}}
            </li>
        </ul>
</nav>

